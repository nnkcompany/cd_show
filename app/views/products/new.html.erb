<!-- <h1>新規投稿</h1> -->
<!-- form_for部分 -->
<%#= form_for(@product) do |f| %>

	<!-- <h4>ジャケット画像</h4> -->
	<%#= f.attachment_field :cd_image %>

    <!-- <h4>アーティスト名</h4> -->
    <%#= f.text_field :artist_name %>

    <!-- <h4>アーティスト名　カナ</h4> -->
    <%#= f.text_field :artist_name_kana %>

    <!-- <h4>アーティスト名　英語</h4> -->
    <%#= f.text_field :artist_name_eng %>

    <!-- <h4>アルバム・シングル名</h4> -->
    <%#= f.text_field :cd_name %>

    <!-- <h4>レーベル</h4> -->
    <%#= f.text_field :label_name %>

    <!-- <h4>レーベル　カナ</h4> -->
    <%#= f.text_field :label_name_kana %>

    <!-- <h4>レーベル　英語</h4> -->
    <%#= f.text_field :label_name_eng %>

    <!-- <h4>プライス</h4> -->
    <%#= f.text_field :price %>

    <!-- <h4>ジャンル</h4> -->
    <%#= f.text_field :category %>

    <!-- <h4>在庫数</h4> -->
    <%#= f.text_field :quantity %>

<!-- <br> -->
<!-- <br> -->
    <%#= f.submit '登録' %>
<!-- <br> -->
<!-- <br> -->

<%# end %>



